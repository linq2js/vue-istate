{
  "name": "vue-istate",
  "version": "0.0.2",
  "description": "A Vue bindings for istate",
  "main": "dist/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "babel src --out-dir dist --source-maps --ignore '**/*.test.js'",
    "_publish": "npm run test && npm run build && npm publish"
  },
  "bugs": {
    "url": "https://github.com/linq2js/vue-istate/issues"
  },
  "homepage": "https://github.com/linq2js/vue-istate#readme",
  "keywords": [
    "scope",
    "functional",
    "context",
    "state management",
    "state manager",
    "redux",
    "state",
    "vue",
    "hook",
    "vuex"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "istate": "^0.0.19",
    "iscope": "^0.0.7"
  },
  "peerDependencies": {},
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest",
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!istate).+\\.js$"
    ]
  },
  "devDependencies": {
    "prettier": "^2.0.5",
    "cross-env": "^7.0.2",
    "@types/jest": "^25.2.3",
    "jest": "^26.0.1",
    "babel-jest": "^26.0.1",
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.7.7",
    "babel-plugin-module-resolver": "^4.0.0",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/test-utils": "^1.0.3",
    "vue-template-compiler": "^2.6.11",
    "vue": "^2.6.11"
  }
}
